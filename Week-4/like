SELECT OrderId, OrderDate, PaymentAmount, StoreName, FirstName, LastName
FROM Orders 
JOIN Customers
ON Customers.CustomerId = Orders.CustomerId
JOIN Stores
ON Stores.StoreId = Orders.StoreId
WHERE StoreName = 'Love You Latte North' 
AND FirstName = 'William' 
AND LastName = 'Jones'
AND OrderDate LIKE '%2024%'
ORDER BY OrderDate DESC
